{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongodbConfiguration": {
    "Connections": [
      {
        "ConnectionString": "mongodb://localhost:27017",
        "Databases": ["ExampleDb","InventoryDb"]
      }
    ]
  },
  "ApiDocsConfig": {
    "ShowSwaggerUi": true,
    "ShowRedocUi": false,
    "EnableSwaggerTryIt": true
  }
}
